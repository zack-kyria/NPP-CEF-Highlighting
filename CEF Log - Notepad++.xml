<NotepadPlus>
    <UserLang name="CEF Logs by zkyria" ext="cef" udlVersion="2.1">
        <!-- Global settings for the language -->
        <Settings>
            <Global caseIgnored="yes" allowFoldOfComments="no" foldCompact="no" forcePureLC="0" decimalSeparator="0" />
            <Prefix Keywords1="no" Keywords2="no" Keywords3="no" Keywords4="no" Keywords5="no" Keywords6="no" Keywords7="no" Keywords8="no" />
        </Settings>

        <!-- Define keywords and other language elements -->
        <KeywordLists>
            <!-- Comments (currently unused but included for compatibility) -->
            <Keywords name="Comments"></Keywords>

            <!-- Numbers (unused but can be added if numeric fields need highlighting) -->
            <Keywords name="Numbers, prefix1"></Keywords>
            <Keywords name="Numbers, prefix2"></Keywords>
            <Keywords name="Numbers, extras1"></Keywords>
            <Keywords name="Numbers, extras2"></Keywords>
            <Keywords name="Numbers, suffix1"></Keywords>
            <Keywords name="Numbers, suffix2"></Keywords>
            <Keywords name="Numbers, range"></Keywords>

            <!-- Operators like | and = for log structure -->
            <Keywords name="Operators1">| = / // \ \\</Keywords>
            <Keywords name="Operators2"></Keywords>

            <!-- Folders (not needed for CEF logs but defined for syntax consistency) -->
            <Keywords name="Folders in code1, open">{</Keywords>
            <Keywords name="Folders in code1, close">}</Keywords>

            <!-- Important fields/keywords -->
            <Keywords name="Keywords1">CEF dhost duser msg message name shost src SourceIP dst DestinationIP spt sourcePort dpt destinationPort deviceProcessName dvc deviceHost dvchost sourceHost destinationHost DeviceAction DeviceSeverity Request_URL suser suid fname categoryDeviceGroup categoryBehavior categoryOutcome categoryObject request cat act filePath user remip dst_host fileHash srcip srcport dstip dstport hostname action url agent srccountry srcintf dstcountry dstintf type policytype catdesc</Keywords>
            <Keywords name="Keywords2">cs1Label cs2Label cs3Label cs4Label cs5Label cs6Label proto cs1 cs2 cs3 cs4 cs5 cs6 cs7 cs7Label cs8 cs8Label cs9 cs9Label cn1 cn1Label cn2 cn2Label cn3 cn3Label cnt c6a1Label</Keywords>
            <Keywords name="Keywords3">ALLOW ALLOWED ACCEPT ACCEPTED DROP DROPPED BLOCK BLOCKED INFO WARN ERROR DEBUG CRITICAL reset-both RESET-BOTH aph_blocked APH_BLOCKED passthrough</Keywords>
            <Keywords name="Keywords4">sourceTranslatedPort sourceTranslatedAddress destinationTranslatedPort destinationTranslatedAddress requestContext deviceOutboundInterface deviceInboundInterface deviceExternalId eventId deviceProcessId outcome agt c6a1 frameworkName rt BitdefenderGZMalwareName BitdefenderGZMalwareHash BitdefenderGZAttackType BitdefenderGZDetectionLevel BitdefenderGZFilelessAttack BitdefenderGZProcessInfoPath BitdefenderGZProcessCommandLineParameters BitdefenderGZParentPid BitdefenderGZComputerFQDN BitdefenderGZParentPath BitdefenderGZHwId BitdefenderGZDetectionTime PanOSContentVer PanOSAssocID PanOSThreatCategory PanOSTunnelType PanOSHTTPHeader PanOSURLCatList PanOSRuleUUID PanOSHTTP2Con PanOSSrcUUID PanOSDstUUID PanOSMonitorTag PanOSTunnelID PanOSParentSessionID PanOSParentStartTime PanOSActionFlags PanOSPPID agentZoneURI ahost amac eventtime dstintfrole dstuuid reqtype profile catdesc</Keywords>
            <Keywords name="Keywords5">flexString1Label flexString1 flexString2Label flexString2 ratemethod devname devid</Keywords>
            <Keywords name="Keywords6"></Keywords>
            <Keywords name="Keywords7"></Keywords>
            <Keywords name="Keywords8"></Keywords>

            <!-- Delimiters for quoted strings -->
            <Keywords name="Delimiters">00&quot; 01 02&quot; 03&apos; 04 05&apos;</Keywords>
        </KeywordLists>

        <!-- Define styles for different language elements -->
        <Styles>
            <!-- Default style -->
            <WordsStyle name="DEFAULT" fgColor="FFFFFF" bgColor="000000" colorStyle="1" fontStyle="0" nesting="0" />

            <!-- Comment styles -->
            <WordsStyle name="COMMENTS" fgColor="00FF80" bgColor="000000" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="LINE COMMENTS" fgColor="00FF80" bgColor="000000" colorStyle="1" fontStyle="0" nesting="0" />

            <!-- Number styles -->
            <WordsStyle name="NUMBERS" fgColor="80FFFF" bgColor="000000" colorStyle="1" fontStyle="0" nesting="0" />

            <!-- Keywords styles -->
            <WordsStyle name="KEYWORDS1" fgColor="FF0000" bgColor="000000" colorStyle="1" fontStyle="1" nesting="0" />
            <WordsStyle name="KEYWORDS2" fgColor="8080FF" bgColor="000000" colorStyle="1" fontStyle="1" nesting="0" />
            <WordsStyle name="KEYWORDS3" fgColor="0000FF" bgColor="000000" colorStyle="1" fontStyle="1" fontSize="14" nesting="0" />
            <WordsStyle name="KEYWORDS4" fgColor="00FFFF" bgColor="000000" colorStyle="1" fontStyle="1" nesting="0" />
            <WordsStyle name="KEYWORDS5" fgColor="808080" bgColor="000000" colorStyle="1" fontStyle="1" nesting="0" />

            <!-- Operator styles -->
            <WordsStyle name="OPERATORS" fgColor="FF00FF" bgColor="000000" colorStyle="1" fontStyle="1" nesting="0" />

            <!-- Folder styles (not applied for CEF logs) -->
            <WordsStyle name="FOLDER IN CODE1" fgColor="000000" bgColor="000000" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="FOLDER IN CODE2" fgColor="000000" bgColor="000000" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="FOLDER IN COMMENT" fgColor="000000" bgColor="000000" colorStyle="1" fontStyle="0" nesting="0" />

            <!-- Delimiter styles -->
            <WordsStyle name="DELIMITERS1" fgColor="FFFF80" bgColor="000000" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS2" fgColor="FFFF80" bgColor="000000" colorStyle="1" fontStyle="0" nesting="0" />
        </Styles>
    </UserLang>
</NotepadPlus>